    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="false">

        </activity>

        <activity
            android:name=".login.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <activity android:name=".announcement.AnnounceRecyclerviewItem"></activity>
        <activity android:name=".announcement.AnnounceListActivity"></activity>
        <activity android:name=".announcement.AnnounceDetailActivity"></activity>
        <activity android:name=".AnnouncePostActivity"></activity>
        <activity android:name=".restaurant.blog.RestaurantPageActivity"></activity>
        <activity android:name=".mypage.profile.MyProfileActivity"></activity>
        <activity android:name=".ram.my_application_1.MyPostActivity"></activity>
        <activity android:name=".commercial.CommercialListActivity"></activity>
        <activity android:name=".commercial.CommercialSignUpActivity"></activity>
        <activity android:name=".ram.my_application_1.MyCommercialDetailActivity"></activity>
        <activity android:name=".ram.my_application_1.current_application.CurrentApplicationActivity"></activity>
        <activity android:name=".ram.chat.ChattingActivity"></activity>
        <activity android:name=".signup.SignUpUserCategoryActivity"></activity>
        <activity android:name=".signup.SignUpUserPersonalInfoActivity"></activity>
        <activity android:name=".news.NewsActivity"></activity>
        <activity android:name=".signup.SignUpActivity"></activity>
        <activity android:name=".signup.SignUpAlarmKeywordActivity"></activity>
        <activity android:name=".signup.SignUpRegionActivity"></activity>
        <activity android:name=".mypage.withdrawal.WithdrawalActivity"></activity>
        <activity android:name=".wishlist.WishListActivity"></activity>
        <activity android:name=".mypage.notice.NoticeActivity"></activity>
        <activity android:name=".announcement.PlaceSearchActivity"></activity>
        <activity android:name=".ram.my_application_1.JoinPostDetailActivity"></activity>
        <activity android:name=".ram.my_application_1.PostRetouchActivity"></activity>
        <activity android:name=".mypage.alarmKeyword.MypageAlarmKeywordActivity"></activity>

        <activity
            android:name=".login.LoginActivity"
            android:exported="false">
        </activity>

        <!-- 23.02.06 제이 추가Kakao 로그인 -->
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- Redirect URI: "kakao{NATIVE_APP_KEY}://oauth" -->
                <!-- scheme-example: "kakao{NATIVE_APP_KEY}" -->
                <data
                    android:host="oauth"
                    android:scheme="@string/kakao_app_key2" />

            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

    </application>



</manifest>